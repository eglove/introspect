{
  "name": "tanstack-router",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Ethan Glover",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "exit 0; vite build src/entry-client.tsx --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "serve": "NODE_ENV=production node server",
    "debug": "node --inspect-brk server"
  },
  "eslintConfig": {
    "extends": "ethang"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.16.6",
    "@tanstack/react-router-server": "^1.16.6",
    "@tanstack/router-devtools": "^1.16.6",
    "@tanstack/router-vite-plugin": "^1.16.5",
    "get-port": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "compression": "^1.7.4",
    "eslint-config-ethang": "^14.0.9",
    "express": "^4.18.2",
    "vite": "^5.1.3"
  }
}
